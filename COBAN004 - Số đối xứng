#include <iostream>
#include <math.h>
using namespace std;
int main () {
	int t;
	cin >> t;
	while (t --) {
		int k;
		cin >> k;
		if (k % 2 != 0) {
			int x = (k - 1) / 2;
			cout << int((pow (10, x) - pow (10, x - 1)) * 10) << endl;
		}		 
		else {
		cout << int(pow (10, k / 2) - pow (10, (k / 2) - 1)) << endl; 
		}
	}
	return 0;
}
